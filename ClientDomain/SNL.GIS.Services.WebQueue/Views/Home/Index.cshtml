@{
    ViewBag.Title = "SNL Maps";
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title"><i class="fa fa-money"></i> Recent Messages</h3>
            </div>
            <div class="panel-body">
                <div id="messages" class="table-responsive">
                    <table class="table table-bordered table-hover table-striped tablesorter">
                        <thead>
                            <tr>
                                <th>Id <i class="fa fa-sort"></i></th>
                                <th>Details <i class="fa fa-sort"></i></th>
                                <th> <i class="fa fa-sort"></i></th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
                <div class="text-right">
                    <a id="more" href="#">More <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
        </div>
    </div>
</div><!-- /.row -->
